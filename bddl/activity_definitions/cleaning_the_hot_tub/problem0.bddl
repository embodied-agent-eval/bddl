        (covered pool.n.01_1 stain.n.01_1)
        (ontop scrub_brush.n.01_1 floor.n.01_1)
        (filled liquid_soap__bottle.n.01_1 liquid_soap.n.01_1)
        (ontop liquid_soap__bottle.n.01_1 floor.n.01_1)
        (insource sink.n.01_1 water.n.06_1)
